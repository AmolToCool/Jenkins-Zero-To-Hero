pipeline {
  agent any
  environment{
    NAME = 'Jay Bhim'
  }
  stages {
    stage('Back-end') {
      steps {
        echo 'Welcome to Goa Singham!!!'
      }
    }
  }
   post{
    always{
      echo "${NAME} Good Bye Singham!!!"
  }
     success{
        echo "Congrates ${NAME} for Success!!!"
     }
}
}
