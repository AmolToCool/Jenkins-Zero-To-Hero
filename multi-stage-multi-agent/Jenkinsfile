pipeline{
  agent any
  parameters{
    string(name: 'JayBhimKambale', defaultValue: 'Sarya Jagachya Mhora Aamhi sari Bhimachi Porha!!!', description: 'Geet')
    booleanParam(name: 'Jayanti', defaultValue: true, description: '14 April')
  }
stages{
  stage('Build'){
      when{
        expression{
            params.Jayanti
        }
      }
    steps{
      echo " Welcome Bhau- ${JayBhimKambale}!!! "
      echo "${Jayanti}"
    }
  }
}
}
